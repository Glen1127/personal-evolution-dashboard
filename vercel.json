{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "outputDirectory": ".next",
    "functions": {
        "pages/api/**/*.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NEXT_PUBLIC_APP_URL": "https://your-app.vercel.app"
    }
}